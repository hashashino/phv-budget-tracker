{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node@3.0.0"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "development-secret-key-please-change-in-production-32-chars",
    "FRONTEND_URL": "https://phv-budget-tracker.vercel.app",
    "ENCRYPTION_KEY": "development-encryption-key-32ch"
  }
}