{
  "version": 2,
  "name": "phv-budget-tracker-backend",
  "installCommand": "npm install --legacy-peer-deps",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "development-secret-key-please-change-in-production-32-chars",
    "FRONTEND_URL": "https://phv-budget-tracker.vercel.app",
    "ENCRYPTION_KEY": "development-encryption-key-32ch"
  }
}