{
  "version": 2,
  "name": "phv-budget-tracker-frontend",
  "buildCommand": "npm run web:build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "EXPO_PUBLIC_API_URL": "https://phv-budget-tracker-backend.vercel.app",
    "API_BASE_URL": "https://phv-budget-tracker-backend.vercel.app"
  }
}